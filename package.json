    "prettier": "prettier --check \"**/*.{js,ts,html}\"",
    "prettier:fix": "prettier --write \"**/*.{js,ts,html,css}\"",
    "stylelint": "stylelint \"src/**/*.css\"",
    "prebuild": "npm run prettier:fix && npm run stylelint && npm run eslint:fix && npm run type-checking",
    "prepare": "husky install"
