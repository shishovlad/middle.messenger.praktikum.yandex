    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "postcss": "^8.4.27",
    "postcss-nested": "^6.0.1",
